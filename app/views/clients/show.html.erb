<div class="col-md-9 col-md-push-4">
  <div class="col-md-12">
    <div class="col-md-6">
      <div class="col-md-4">
        <p class="title"> DNI: </p>
      </div>
      <div class="col-md-8">
        <p class=""> <%= @client.dni %> </p>
      </div>
    </div>
  </div>

   <div class="col-md-12">
    <div class="col-md-6">
      <div class="col-md-4">
        <p class="title"> Nombre: </p>
      </div>
      <div class="col-md-8">
        <p class=""> <%= @client.first_name %> </p>
      </div>
    </div>
  </div>

  <div class="col-md-12">
    <div class="col-md-6">
      <div class="col-md-4">
        <p class="title"> Apellido: </p>
      </div>
      <div class="col-md-8">
        <p class=""> <%= @client.last_name %> </p>
      </div>
    </div>
  </div>
</div>

<div class="col-md-6">
  <div class="col-md-6 col-md-push-6">
    <%= link_to 'Atras', clients_path, {class: 'btn btn-primary btn-block'} %>
  </div>
</div>

<div class="col-md-6">
  <div class="col-md-6 col-md-push-1">
    <%= link_to 'Listar Polizas', client_insurances_path(client_id: @client), {class: 'btn btn-primary btn-block'} %>
  </div>
</div>